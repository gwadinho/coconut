<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-4 col-sm-offset-4">
        <br>
        <h1> Add my island</h1>

        <%= simple_form_for(@island) do |f| %>
          <%= f.input :name %>
          <%= f.input :location %>
          <%= f.input :capacity %>
          <%= f.input :price %>
          <%= f.input :photo %>
          <%= f.input :photo_cache, as: :hidden %>
          <%= f.submit "Create Island", class: "btn btn-success" %>
        <% end %>
    </div>
  </div>
</div>
